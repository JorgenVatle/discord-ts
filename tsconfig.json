{
  "compilerOptions": {
    "target": "es6",
    "rootDirs": [
      "src",
      "types"
    ],
    "outDir": "dist",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "lib": [
      "es6"
    ],
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "types": [],
    "forceConsistentCasingInFileNames": true
  }
}
